/* =Clear Floated Elements
--------------------------------------------------------------------------------*/

/* http://sonspring.com/journal/clearing-floats */

#iavouch_3 * span.clear,
#iavouch_3  * div.clear,
#iavouch_3  * li.clear,
#iavouch_3  * dd.clear
{
	background: none;
	border: 0;
	clear: both;
	display: block;
	float: none;
	font-size: 0;
	list-style: none;
	margin: 0;
	padding: 0;
	overflow: hidden;
	visibility: hidden;
	width: 0;
	height: 0;
}

/* http://www.positioniseverything.net/easyclearing.html */

#iavouch_3 .clearfix:after
{
	clear: both;
	content: '.';
	display: block;
	visibility: hidden;
	height: 0;
}

#iavouch_3 .clearfix
{
	display: inline-block;
}

#iavouch_3 * .clearfix
{
	height: 1%;
}

#iavouch_3 .clearfix
{
	display: block;
}

